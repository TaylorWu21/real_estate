<%= form_for [@seller, @home] do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title %>

	<%= f.label :price %>
	<%= f.number_field :price %>

	<%= f.label :square_feet %>
	<%= f.number_field :square_feet %>

	<%= f.label :bathroom %>
	<%= f.text_field :bathroom %>

	<%= f.label :room %>
	<%= f.text_field :room %>

	<%= f.label :year_built %>
	<%= f.text_field :year_built %>

	<%= f.label :sold %>
	<%= f.check_box :sold %>

	<%= f.submit class:"btn btn-primary"%>

<% end %>